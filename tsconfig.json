{
    "compilerOptions": {
      "module": "commonjs",
      "declaration": false,
      "esModuleInterop": true,
      "removeComments": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "allowSyntheticDefaultImports": true,
      "target": "es5",
      "sourceMap": true,
      "outDir": "./dist",
      "baseUrl": "./",
      "keyofStringsOnly": true
    },
    "exclude": [
      "node_modules",
      "dist"
    ]
  }